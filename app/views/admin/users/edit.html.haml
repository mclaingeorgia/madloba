- title = t('admin.shared.edit_model', name: @model.human)
- redirect_default = true unless redirect_default == true || redirect_default == false
- item = @item || item
- if redirect_default
  - page_title title
  - path = manage_users_path
  - url = manage_user_path(id: item.id)
- else
  - path = manage_user_profile_path(page: :'manage-profile')
  - url = manage_update_user_path
- if redirect_default
  = render partial: 'shared/breadcrumbs', locals: { paths: [[t('admin.shared.manage_model', name: @model.human(2)), path], title] }
= render partial: 'admin/users/form', locals: { obj: [:manage, item], redirect_default: redirect_default, url: url }
